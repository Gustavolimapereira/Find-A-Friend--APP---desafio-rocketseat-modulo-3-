generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Org {
  id              String   @id @default(uuid())
  name_org        String
  responsible_org String
  email           String   @unique
  cep             String
  address         String
  phone           String
  password_hash   String
  created_at      DateTime @default(now())

  @@map("org")
}
